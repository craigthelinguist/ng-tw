{
  "name": "ng-tw",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
		"release:major": "cd ./scripts && npm run release:major",
		"release:minor": "cd ./scripts && npm run release:minor",
		"release:patch": "cd ./scripts && npm run release:patch",
		"build:dev": "./scripts/build.sh",
		"build:prod": "./scripts/build-prod.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "lodash": "^4.17.21",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.2",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.18",
    "typescript": "~4.5.2"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 140,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "disableLanguages": [
      "html",
      "css",
      "scss"
    ],
    "formatOnSave": false
  }
}
