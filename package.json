{
  "name": "ng-tw",
  "version": "0.0.14",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proejct ng-tw",
    "build": "ng build --proejct ng-tw",
    "watch": "ng build --project ng-tw --watch --configuration development",
    "test": "ng test",
    "release:major": "cd ./scripts && npm run release:major",
    "release:minor": "cd ./scripts && npm run release:minor",
    "release:patch": "cd ./scripts && npm run release:patch",
    "build:dev": "./scripts/build.sh",
    "build:prod": "./scripts/build-prod.sh",
    "dev:ssr": "ng run sandbox:serve-ssr",
    "serve:ssr": "node dist/sandbox/server/main.js",
    "build:ssr": "ng build && ng run sandbox:server",
    "prerender": "ng run sandbox:prerender",
    "sandbox:release": "ng run sandbox:prerender --configuration=production && ng deploy --no-build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^13.3.1",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/platform-server": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@nguniversal/express-engine": "^13.0.2",
    "express": "^4.15.2",
    "lodash": "^4.17.21",
    "ngx-google-analytics": "^13.0.1",
    "ngx-markdown": "^13.1.0",
    "prismjs": "^1.27.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.2.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.18",
    "typescript": "~4.8.4"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 140,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "disableLanguages": [
      "html",
      "css",
      "scss"
    ],
    "formatOnSave": false
  }
}